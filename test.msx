// vim: ts=2 sw=2 expandtab ft=java

voice "trumpet" {
  file "trumpet.wav"
}

voice "brass" {
  pitch a
  file "brass.wav"
}

channel 0 {
  [tempo 150, unit 1/8, voice "brass"]
  {times 3:
    a8 c2 R6
    {with [tempo +20]: c d/2 e/2}
    {on 3: c2 d4 f#2}
  }
}

channel 1 {
  [tempo 150, unit 1/4, voice "trumpet"]
  {times 5: b, C D' R}
}
